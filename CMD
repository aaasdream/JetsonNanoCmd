rosrun ORB_SLAM3 Mono /home/aking/orbslam3/ORB_SLAM3/Vocabulary/ORBvoc.txt  ~/orbslam3/ORB_SLAM3/Examples/Monocular/TUM1.yaml
roslaunch jetson_camera aking_cam1.launch 
